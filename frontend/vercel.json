{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "installCommand": "npm install",
    "functions": {
        "app/api/**/*.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "env": {
        "NEXT_PUBLIC_API_URL": "@NEXT_PUBLIC_API_URL"
    }
}